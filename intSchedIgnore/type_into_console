intro = $(a.binList().items()[0]).control()
intro1 = $(intro.binList().items()[0]).control()
intro2 = $(intro.binList().items()[1]).control()
systems = $(intro.binList().items()[2]).control()
theory = $(intro.binList().items()[3]).control()
$('body').append(a)

